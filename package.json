{
  "name": "react-auto-indexed",
  "version": "1.0.3",
  "description": "Enhance textareas with automatic line numbering and array value handling for better organization and reference.",
  "main": "./dist/index.js",
  "scripts": {
  "build:js": "babel index.js -d dist --presets=@babel/preset-react",
  "build:css": "copy styles.css dist",
  "build": "npm run build:js && npm run build:css",
  "prepublishOnly": "npm run build"
},
  "keywords": [
    "react",
    "numbering",
    "separation",
    "textarea",
    "line numbering",
    "array",
    "indexing",
    "value handling"
  ],
  "author": "davidwale",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidwale/react-auto-indexed.git"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/plugin-syntax-jsx": "^7.23.3",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3"
  }
}
